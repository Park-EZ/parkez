  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"backend,frontend\" -c \"cyan,magenta\" \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "cd server && npm run dev",
    "dev:frontend": "vite",
    "dev:backend": "cd server && npm run dev",
    "kill:3000": "node scripts/kill-port.js 3000",
    "kill:5173": "node scripts/kill-port.js 5173",
    "kill:port": "node scripts/kill-port.js",
    "kill:all": "npm run kill:3000 && npm run kill:5173",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
